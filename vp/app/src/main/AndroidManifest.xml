<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cloud"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Vp">
        <activity android:name=".NotifyActivity"></activity>
        <activity android:name=".Notify_infoActivity" />
        <activity android:name=".NotifylistActivity" />
        <activity android:name=".GroupActivity" />
        <activity android:name=".Big_imageActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LeaveinfoActivity" />
        <activity android:name=".LeavelistActivity" />
        <activity android:name=".LeaveActivity" />
        <activity android:name=".Sign_inActivity" />
        <activity android:name=".Create_ClassActivity" />
        <activity android:name=".TeacherActivity" />
        <activity android:name="com.google.zxing.activity.CaptureActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".ClassinfoActivity" />
        <activity android:name=".ClasslistActivity" />
        <activity android:name=".PlayerActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>